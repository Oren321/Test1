local Toggle = Tabs.AutoFish:AddToggle("AutoFish", 
{
    Title = "Auto Fish Default", 
    Description = "",
    Default = false,
    Callback = function(state)
        getgenv().toggle = state
        
        if state then
            spawn(function()
                while getgenv().toggle do
                  game:GetService("ReplicatedStorage").Network.Fishing_Fish:InvokeServer("Default")
                  game:GetService("ReplicatedStorage").Network.Fishing_Success:FireServer()
                    task.wait()
                end
            end)
        else
            getgenv().toggle = false
        end
    end 
})

