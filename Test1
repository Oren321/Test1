--AutoBuy Fishing Merchants Section
local Section = Tabs.AutoFish:AddSection("AutoBuy Fishing Merchants")

--AutoBuy
local Toggle = Tabs.AutoFish:AddToggle("AutoFish", 
{
    Title = "AutoBuy Default Fishing Merchant", 
    Description = "",
    Default = false,
    Callback = function(state)
        if state then
            if not getgenv().AutoBuyStandardMerchant then
                getgenv().AutoBuyStandardMerchant = true
                spawn(function()
                    while getgenv().AutoBuyStandardMerchant do
                        for i = 1, 6 do
                            for j = 1, 3 do
                                local args = {
                                    [1] = "StandardMerchant",
                                    [2] = i
                                }
                                game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("CustomMerchants_Purchase"):InvokeServer(unpack(args))
                            end
                        end
                        task.wait(30)
                    end
                end)
            end
        else
            getgenv().AutoBuyStandardMerchant = false
        end
    end
})

